{
  "name": "Group Order Extension",
  "version": "1.0",
  "description": "Let us split the bill.",
  "author": "Douglas Berkley",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/scraper.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Group Order"
  }
}
